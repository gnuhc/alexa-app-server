    "lib": ["es6", "es7", "dom", "esnext.asynciterable"],
    "module": "commonjs",
    "noImplicitReturns": true,
    "outDir": "lib",
    "sourceMap": true,
    "target": "es6",
    "noImplicitAny": false,
    "skipLibCheck": true
